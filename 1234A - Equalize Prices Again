package codeforce;
import java.util.Scanner;
public class EqualizePricesAgain 
{
	public static void main(String[] args) 
    {
		double b,s,x,c;
		Scanner in = new Scanner(System.in);
	    int a = in.nextInt();
	    for(int i=1;i<=a;i++)
	    {
	    	s=0;
	    	b = in.nextInt();
	    	for(x=0;x<b;x++)
	    		s=s+in.nextInt();
	    	System.out.println((int)Math.ceil(s/b));
	    }
	}

}
