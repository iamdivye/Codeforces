import java.util.*;
public class SquareString 
{
	public static void main(String[] args) 
	{
		int i,j;
		String k;
		Scanner in = new Scanner(System.in);
		int n= in.nextInt();
		for(i=1;i<=n;i++)
		{
			k = in.next();
			j = k.length()/2;
			if(k.substring(0,j).equals(k.substring(j)))
				System.out.println("YES");
			else
				System.out.println("NO");
		}
	}
}
