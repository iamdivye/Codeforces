    import java.util.Scanner;
    public class DieRoll 
    {
    	public static void main(String[] args) 
    	{
    		Scanner in = new Scanner(System.in);
    		int n = in.nextInt();
    		int m = in.nextInt();
    		n = Math.max(n, m);
    		n = 7-n;
    		if(6%n == 0)
    			System.out.println("1/"+(6/n));
    		else if(n == 4)
    			System.out.println("2/3");
    		else
    		    System.out.println(n+"/"+6);
    	}
    }
