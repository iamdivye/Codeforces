import java.util.*;
public class BuyaShovel {

	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		int a = in.nextInt();
		int k = in.nextInt();
		int x=0,n;
		for(int i=1;i<=9;i++)
		{
			n=a*i;
			if(n%10 == 0 || n%10 == k)
			{
				x++;
				break;
			}
			x++;
		}
		System.out.println(x);
	}

}
