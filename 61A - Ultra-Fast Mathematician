import java.util.Scanner;
public class UltraFastMathematician 
{
	public static void main(String[] args) 
    {
		Scanner in = new Scanner(System.in);
	    String a = in.next();
	    String b = in.next();
	    String s="";
	    for(int i=0;i<a.length();i++)
	    {
	    	if(a.charAt(i) == b.charAt(i))
	    		s = s+"0";
	    	else
	    		s = s+"1";	    		
	    }
	    System.out.println(s);
	}
}
