import java.util.*;
public class MinutesBeforeTheNewYear 
{
	public static void main(String[] args) 
	{
		int i,h,m,t;
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		for(i=0;i<n;i++)
		{
			h = in.nextInt();
			m = in.nextInt();
			t = (23-h)*60 + (60-m);
			System.out.println(t);
		}
	}
}
