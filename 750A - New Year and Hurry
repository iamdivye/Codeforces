import java.util.*;
public class NewYearandHurry {

	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int k = in.nextInt();
		k = 240 - k;
		int c=0;
		for(int i=1;i<=n;i++)
		{
			if(i*5 <= k)
			{
				c++;
				k-=i*5;
			}
			else 
				break;
		}
		System.out.println(c);
	}

}
