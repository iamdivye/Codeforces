import java.util.*;
public class ErasingZero 
{
	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		int t = in.nextInt();
		String s;
		int c=0,j;
		for(int i=0;i<t;i++)
		{
			c=0;
			s = in.next();
			if(s.contains("1"))
			{
			for(j=s.indexOf("1");j<=s.lastIndexOf("1");j++)
			{
				if (s.charAt(j) == '0') 
	            {
	                    c++;
	            }
			}
			}
			System.out.println(c);
		}
	}
}
