import java.util.Scanner;

public class OmkarAndCompletion {

	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		int a = in.nextInt(),x,y,z;
		String s;
	    for(int i=0;i<a;i++)
	    {
	    	s="";
	    	x=in.nextInt();
	    	for(y=1;y<x;y++)
	    	{
	    		s+="1 ";
	    	}
	    	if(x != 0)
	    		System.out.println(s+"1");
	    }
	}

}
