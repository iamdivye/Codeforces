import java.util.Scanner;
public class YESOrYES 
{
	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		String[] S = new String[n];
		for(int i=0; i<n; i++)
		{
			S[i] = in.next().toUpperCase();			
		}
		for(int i=0; i<n; i++)
		{
			if(S[i].equals("YES"))
				System.out.println("YES");
			else
				System.out.println("NO");
		}
	}
}
