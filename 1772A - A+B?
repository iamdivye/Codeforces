import java.util.Scanner;
public class AB 
{
	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		int t = in.nextInt();
		String k;int a,b;
		for(int i=0;i<t;i++)
		{
			k = in.next();
			a = Integer.parseInt(""+k.charAt(0));
			b = Integer.parseInt(""+k.charAt(2));
			System.out.println(a+b);
		}
	}
}
