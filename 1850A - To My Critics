import java.util.Scanner;
public class ToMyCritics 
{
	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int a,b,c;
		for(int i=0;i<n;i++)
		{
			a = in.nextInt();
			b = in.nextInt();
			c = in.nextInt();
			if((a+b) >= 10 || (b+c) >= 10 || (c+a) >= 10)
				System.out.println("YES");
			else
				System.out.println("NO");				
		}
	}

}
