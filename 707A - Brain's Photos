import java.util.*;
public class BrainsPhotos 
{
	public static void main(String[] args)
	{
		int i,j,a=0;
		char k;
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int m = in.nextInt();
		for(i=0;i<n;i++)
		{
			for(j=0;j<m;j++) 
			{
				k = in.next().charAt(0);
				if(k != 'B' && k != 'W' && k != 'G')
					a++;
			}
		}
		if(a == 0)
			System.out.println("#Black&White");
		else
		    System.out.println("#Color");
	}
}
