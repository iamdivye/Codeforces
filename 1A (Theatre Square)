import java.util.*;
public class TheatreSquare 
{
	public static void main(String[] args) 
	{
		 Scanner in = new Scanner(System.in);
	       int n = in.nextInt();
	       int m = in.nextInt();
	       int a = in.nextInt();
	       int rows = (n + a - 1)/a; 
	       int cols = (m + a - 1)/a;
	       long tF = (long) rows * cols;
	       System.out.println(tF);
	}
}
