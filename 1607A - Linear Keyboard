package codeforce;
import java.util.*;
public class LinearKeyboard 
{
	public static void main(String[] args) 
	{
		int i,j,k,l,m=0;
		String s,c;
		char x,y,z;
		Scanner in = new Scanner(System.in);
		int t = in.nextInt();
		for(i=0;i<t;i++)
		{
			m=0;
			s = in.next();
			c = in.next();
			k = s.indexOf(c.charAt(0));
			for(j=1;j<c.length();j++)
			{
				l = s.indexOf(c.charAt(j));
				m = m + Math.abs(k-l);
				k = l;				
			}
			System.out.println(m);
		}
	}
}
