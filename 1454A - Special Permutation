import java.util.Scanner;

public class SpecialPermutation {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
	    int a = in.nextInt();
	    int x,y;
	    for(int i=0;i<a;i++)
	    {
	    	x=in.nextInt();
	    	for(y=2;y<=x;y++)
	    		System.out.print(y+" ");
	    	System.out.println(1);
	    }
	}

}
