import java.util.*;
public class StairPeakorNeither {

	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		int t = in.nextInt();
		for(int i=0;i<t;i++)
		{
			int a = in.nextInt();
			int b = in.nextInt();
			int c = in.nextInt();
			if(c>b && b>a)
				System.out.println("STAIR");
			else if(b>a && b>c)
				System.out.println("PEAK");
			else
				System.out.println("NONE");
		}
	}

}
