import java.util.*;
public class VusTheCossackAndAContest 
{
	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int a = in.nextInt();
		int b = in.nextInt();
		if(a>=n && b>=n)
			System.out.println("YES");
		else
			System.out.println("NO");
	}
}
