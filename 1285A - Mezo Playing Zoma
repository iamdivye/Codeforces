import java.util.*;
public class MezoPlayingZoma 
{
	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		int k = in.nextInt();
		String s = in.next();
		int l=0,r=0;
		for(int i=0;i<s.length();i++)
		{
			if(s.charAt(i) == 'L')
				l++;
			else
				r++;
		}
		System.out.println(l+r+1);
	}
}
