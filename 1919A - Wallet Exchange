import java.util.*;
public class WalletExchange 
{
	public static void main(String[] args) 
	{
		int k;
		Scanner in = new Scanner(System.in);
		int z = in.nextInt();
		for(k=0;k<z;k++)
		{
			int a = in.nextInt();
			int b = in.nextInt();
			
			if((a+b)%2 == 0)
				System.out.println("Bob");
			else
				System.out.println("Alice");
		}
	}
}
