import java.util.*;
public class CreatingWords {

	public static void main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		for(int i=0;i<n;i++)
		{
			String a=in.next();
			String b=in.next();
			char e = a.charAt(0);
			char f = b.charAt(0);
			String c = f + a.substring(1);
			String d = e + b.substring(1);
			System.out.println(c+" "+d);
		}
	}

}
